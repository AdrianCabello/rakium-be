meta {
  name: Get Published Project (Public)
  type: http
  seq: 6
}

get {
  url: {{baseUrl}}/projects/{{projectId}}/published
  body: none
  auth: none
}

vars:pre-request {
  baseUrl: https://rakium-be-production.up.railway.app
  projectId: bruno.getEnvironmentVar("projectId")
}

tests {
  test("Status code is 200", function() {
    expect(res.getStatus()).to.equal(200);
  });
  
  test("Project is published", function() {
    expect(res.getBody().status).to.equal("PUBLISHED");
  });
}
